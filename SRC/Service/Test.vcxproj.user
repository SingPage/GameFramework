<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <DebuggerFlavor>LinuxDebugger</DebuggerFlavor>
    <RemoteDebuggerWorkingDirectory>$(RemoteRootDir)/bin/x64/Debug/</RemoteDebuggerWorkingDirectory>
    <RemoteDebuggerCommand>$(RemoteRootDir)/bin/x64/Debug/$(TargetFileName)</RemoteDebuggerCommand>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|ARM'">
    <RemoteDebuggerWorkingDirectory>$(SolutionDir)bin\$(Platform)\$(Configuration)\</RemoteDebuggerWorkingDirectory>
    <DebuggerFlavor>LinuxDebugger</DebuggerFlavor>
  </PropertyGroup>
</Project>